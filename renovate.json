{
  "extends": [
    "config:base"
  ],
  "regexManagers": [
    {
      "fileMatch": ["^Dockerfile$"],
      "matchStrings": [
        "^ARG DATADOG_VERSION=(?<currentValue>.*) # (?<datasource>.*?)/(?<depName>.*?)(\\&versioning=(?<versioning>.*?))?\\s"
      ],
      "datasourceTemplate": "github-tag",
      "depNameTemplate": "DataDog/datadog-agent",
      "versioningTemplate": "semver"
   }
  ]
}
